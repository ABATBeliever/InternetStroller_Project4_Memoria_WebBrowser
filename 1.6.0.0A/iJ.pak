;InternetStroller Memoria ISConfig.ini
[HEAD]
HEAD_VERSION=2
HEAD_WRITER=ABATBeliever
HEAD_LANGUAGE=JAPANESE-日本語
HEAD_BUILD=2024/08/06

[REG]
;Weft Size (Default=>1200)
REG_WINDOW1=1200
;Warp Size (Default=>600)
REG_WINDOW2=600
;Weft Position (Reset=>'')
REG_WINDOWPOS1=
;Warp Position (Reset=>'')
REG_WINDOWPOS2=
REG_DarkMode=0
REG_CLOSEANIMATION=1
REG_ISCRIPTENABLE=0
;MDI
REG_MDI=1
REG_DPI=0
;UserAgent (Default=>'')
REG_DEFUA=
REG_STBAR=0
;DefalutPageColor (Default=>'' #<BackGround>,<Text>,<Section BackGround> RGB)
REG_DEFPAGECOLOR=
REG_AUTOSEARCH=0

[URLs]
;Search URL(MANA PAD)
;First URL(Original:"include")
